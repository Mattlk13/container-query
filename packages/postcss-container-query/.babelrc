{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "node": 4,
          "browsers": [
            "IE >= 10",
            "Chrome >= 10",
            "Opera >= 15",
            "FF >= 10",
            "Edge >= 12"
          ]
        }
      }
    ]
  ],
  "plugins": ["transform-flow-strip-types", "lodash"],
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": 4
            }
          }
        ]
      ]
    }
  }
}
