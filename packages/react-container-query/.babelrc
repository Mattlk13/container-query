{
  "presets": [
    "react",
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "IE >= 10",
            "Chrome >= 10",
            "Opera >= 15",
            "FF >= 10",
            "Edge >= 12"
          ]
        }
      }
    ]
  ],
  "plugins": ["external-helpers", "transform-object-rest-spread"]
}
